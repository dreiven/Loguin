<script type="text/javascript">
$(document).ready(function(){
    var counterPanel = $('.counter-panel .info .counter');
    var captchaPanel = $('.counter-panel .info .captcha');
    var counter =  $('.counter-panel .info .counter .time');
    var button = $('.counter-panel .button .get-premium-button');
    var captchaForm = $('#captchasubmitform');
    
    var fileIdhdn = $('<input />');
        fileIdhdn.attr('type', 'hidden');
        fileIdhdn.attr('id', 'request_fileId_field');
        fileIdhdn.val('TVRFM01EYzVPVGsF9');
        fileIdhdn.appendTo(captchaForm); 

    //setting up
    captchaForm.on('submit', function(){ return false; });
    captchaPanel.hide();
    counterPanel.show();

    button.click(function(e){
        e.preventDefault();

        if($(this).hasClass('disabled')){
            return false;
        }

        if (captchaPanel.is(":hidden"))
        {
            counterPanel.hide();
            captchaPanel.show();
            return;
        }

        $(this).validateCaptcha(captchaForm, function(responce){

            captchaPanel.hide();
            button.hide();
            counterPanel.show();


            if(responce != null && responce.token){
                var hdn = $('<input />');
                hdn.attr('type', 'hidden');
                hdn.attr('id', 'responce_token_field');
                hdn.val(responce.token);
                hdn.appendTo(captchaForm);
            }

            counter.contdownTimer('1200', function(){

                $(this).getFileDownloadLink('TVRFM01EYzVPVGsF9', captchaForm, function(link){
                                                window.location.href = link;
                                            }, function(msg){
                                                showDialogError(msg);
                                            });
            });

        }, function(msg){showDialogError(msg)});
    });
});

function showDialogError(msg){

    var dialog = $('<div />').html('<p class="dialog-error-message">' +msg +'</p>' );

    var title = 'CAPTCHA is not valid';

    if (msg == "Download file count limit.")
    {
        title = 'Downloads count limit';
    }

    dialog.dialog({
        autoOpen: true,
        modal: true,
        title: title,
        buttons: [{
            text: "OK",
            click: function(e) {

                if (msg == "Download file count limit.")
                {
                    window.location.href = window.location.href;
                }
                else
                {
                    $(this).dialog('close');
                    $(this).dialog('remove');
                }
            },
            'class': 'orange'
        }]
    });

}

</script>

